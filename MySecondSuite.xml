<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="MyFirstSuite">
	
	<test name="Google Search on Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://google.com.ar" />

		<groups>
			<run>
				<exclude name="brokenTest"></exclude>
				<include name="setup"></include>
				<include name="smoke"></include>
			</run>
		</groups>
		<packages>
			<package name="ar.com.informatorio.calidad.tests" />
		</packages>
	</test>
</suite>